package com.tly.model;

import java.util.Date;


/**
 * 合伙人设备对应
 * @ClassName PartnerEquipment
 * @Author yangweihang
 * @Date 2018年8月24日 上午11:25:37
 */
public class PartnerEquipment {
	private Integer id;//id
	private Integer pid;//合伙人id
	private Integer eid;//设备子表id
	private Integer status;//设备状态   1.离线   2.待使用     3.使用中   4.故障
	private Integer form;//使用形式 1.租用 2.购买
	private Date time;//验收时间
	private Double smoney;//扫码价格
	private String name;//设备命名
	private Integer look;//1.已读 2.未读
	public PartnerEquipment() {
	}
	public PartnerEquipment(Integer id, Integer pid, Integer eid, Integer status, Integer form, Date time,
			Double smoney, String name, Integer look) {
		this.id = id;
		this.pid = pid;
		this.eid = eid;
		this.status = status;
		this.form = form;
		this.time = time;
		this.smoney = smoney;
		this.name = name;
		this.look = look;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getPid() {
		return pid;
	}
	public void setPid(Integer pid) {
		this.pid = pid;
	}
	public Integer getEid() {
		return eid;
	}
	public void setEid(Integer eid) {
		this.eid = eid;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public Integer getForm() {
		return form;
	}
	public void setForm(Integer form) {
		this.form = form;
	}
	public Date getTime() {
		return time;
	}
	public void setTime(Date time) {
		this.time = time;
	}
	public Double getSmoney() {
		return smoney;
	}
	public void setSmoney(Double smoney) {
		this.smoney = smoney;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getLook() {
		return look;
	}
	public void setLook(Integer look) {
		this.look = look;
	}
	@Override
	public String toString() {
		return "PartnerEquipment [id=" + id + ", pid=" + pid + ", eid=" + eid + ", status=" + status + ", form=" + form
				+ ", time=" + time + ", smoney=" + smoney + ", name=" + name + "]";
	}
}
